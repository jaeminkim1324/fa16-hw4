<table>
	<tr>
		<th>Task</th>
		<th>Done?<th>
	</tr>
<% @todos.each do |data| %>
	<tr>
		<td><%= data.tasks %></td>
		<td><%= data.finished %></td>
	</tr>
<% end %>
</table>

<table>
	<tr>
		<th>Cat Name</th>
	</tr>
<% @cats.each do |cat| %>
	<tr>
		<td><%= cat.name %></td>
	</tr>
<% end %>
</table>


<table>
	<tr>
		<th>Username</th>
		<th>Email<th>
		<th>Age<th>
	</tr>
<% @users.each do |user| %>
	<tr>
		<td><%= user.username %></td>
		<td><%= user.email %></td>
		<td><%= user.age %></td>
	</tr>
<% end %>
</table>